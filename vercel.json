{
  "version": 2,
  "name": "cpi-retail-benchmark",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}
