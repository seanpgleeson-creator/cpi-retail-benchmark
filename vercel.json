{
  "version": 2,
  "name": "cpi-retail-benchmark",
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "NODE_VERSION": "18"
  },
  "functions": {
    "app/main.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}
